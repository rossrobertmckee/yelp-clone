<div class="col-xs-offset-1 col-xs-10 booyah-box">
  <h1> Enter a new Place</h1>
  <br />

  <%= simple_form_for @place do |f| %>
    <%= f.input :name %>
    <br />
    <%= f.input :address %>
    <br />
    <%= f.input :description %>
    <br />
    <%= f.submit 'Create', :class => 'btn btn-primary btn-lg pull-right' %>
  <% end %>
</div>
