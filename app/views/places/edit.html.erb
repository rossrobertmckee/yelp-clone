<div class="col-xs-offset-1 col-xs-10 booyah-box">
  <h1> Update Your Place</h1>
  <br />

<%= simple_form_for current_place do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :latitude %>
  <%= f.input :longitude %>
  <%= f.input :address %>
  <%= f.submit 'Update!', :class => 'btn btn-primary btn-lg pull-right' %>
<% end %>

